ashrafessa@ashrafessa-VirtualBox:~/Desktop$ mkdir Practice03
ashrafessa@ashrafessa-VirtualBox:~/Desktop$ cd Practice03/
ashrafessa@ashrafessa-VirtualBox:~/Desktop/Practice03$ touch first.txt
ashrafessa@ashrafessa-VirtualBox:~/Desktop/Practice03$ chmod 666 first.txt 
ashrafessa@ashrafessa-VirtualBox:~/Desktop/Practice03$ mv first.txt one.txt
ashrafessa@ashrafessa-VirtualBox:~/Desktop/Practice03$ chmod 600 one.txt
ashrafessa@ashrafessa-VirtualBox:~/Desktop/Practice03$ ls -l
ashrafessa@ashrafessa-VirtualBox:~/Desktop/Practice03$ mkdir FolderToDelete
ashrafessa@ashrafessa-VirtualBox:~/Desktop/Practice03$ cd FolderToDelete/
ashrafessa@ashrafessa-VirtualBox:~/Desktop/Practice03/FolderToDelete$ touch a.txt b.txt c.txt
ashrafessa@ashrafessa-VirtualBox:~/Desktop/Practice03/FolderToDelete$ cd ..
ashrafessa@ashrafessa-VirtualBox:~/Desktop/Practice03$ rm -r FolderToDelete/
